    "#7.check array is monotonic or not\n",
    "def isMonotonic(arr):\n",
    "    return (all(arr[i] <= arr[i + 1] for i in range(len(arr) - 1)) or\n",
    "      all(arr[i] >= arr[i + 1] for i in range(len(arr) - 1)))\n",
    "arr=[1,2,3,4,5]\n",
    "print(isMonotonic(arr))"
   ]
  }
